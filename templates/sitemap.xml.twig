<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

<url><loc>{{urls.siteURL()}}</loc><priority>1</priority></url>

<!-- products -->
{% for product in products -%}
	<url><loc>{{urls.productURL(product)}}</loc><priority>0.8</priority></url>
{% endfor %}

<!-- pages -->
{% for page in pages -%}
	<url><loc>{{urls.pageURL(page)}}</loc><priority>0.7</priority></url>
{% endfor %}

</urlset>
