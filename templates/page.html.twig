{% set ogtype='page' %}

{% block main %}
	<section class="center pa3 cf">
		<header>
			<h2 class="purple f3">{{page.name}}</h2>
		</header>
		<main class="f5">
		{{ page.content|raw }}
		</main>
	</section>
{% endblock %}
